{
    "name": "moneymaxim/trustpilot-invitation-api",
    "description": "A PHP library for accessing the Trustpilot Invitation API",
    "repositories":
    [
        {
            "type": "vcs",
            "url": "http://github.com/proggeler/TrustpilotAuthenticator"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "guzzlehttp/guzzle": "^7.0",
        "moneymaxim/trustpilot-authenticator": "dev-guzzle-update as 0.2.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Andrew Carter",
            "email": "andrew.carter@moneymaxim.co.uk"
        }
    ],
    "autoload": {
        "psr-4": {
            "Trustpilot\\Api\\Invitation\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Trustpilot\\Test\\Api\\Invitation\\": "test/"
        }
    }
}

